{
  "name": "astro-starter",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update:deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "preview": "astro preview",
    "i18n": "astro-i18next generate",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.1",
    "@astrojs/vue": "^3.0.0",
    "@unocss/reset": "^0.55.7",
    "astro": "^3.1.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.0-beta.0",
    "@astrojs/netlify": "^3.0.1",
    "@astrojs/node": "^6.0.0",
    "@astrojs/sitemap": "^3.0.0",
    "@astrojs/vercel": "^5.0.0",
    "@eiog/eslint-config-ts": "^0.0.2",
    "@vite-pwa/astro": "^0.1.3",
    "@vitest/ui": "^0.34.4",
    "@vue/test-utils": "^2.4.1",
    "astro-i18next": "1.0.0-beta.21",
    "astro-icon": "^0.8.1",
    "astro-robots-txt": "^1.0.0",
    "commitlint": "^17.7.1",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.49.0",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "postcss-preset-env": "^9.1.4",
    "sass": "^1.67.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
